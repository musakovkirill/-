import sqlite3 as sq

conn1 = sq.connect('test_db.sqlite')
# cursor = conn.cursor()
cursor1 = conn1.cursor()
cursor1.execute('''CREATE TABLE IF NOT EXISTS warehouse (
                   id INTEGER NOT NULL,
                   address TEXT NOT NULL,
                   product TEXT NOT NULL,
                   FOREIGN KEY);''')


conn1 = sq.connect('test_db.sqlite')
# cursor = conn.cursor()
cursor1 = conn1.cursor()
cursor1.execute('''CREATE TABLE IF NOT EXISTS product (
                   id INTEGER NOT NULL,
                   price TEXT NOT NULL,
                   numbers TEXT NOT NULL);''')
               


conn1 = sq.connect('test_db.sqlite')
# cursor = conn.cursor()
cursor1 = conn1.cursor()
cursor1.execute('''CREATE TABLE IF NOT EXISTS orders ,(id INTEGER PRIMARY KEY AUTOINCREMENT,
                 id warehouse INTEGER NOT NULL
                 address warehouse TEXT NOT NULL,
                 place INTEGER NOT NULL
                   FOREIGN KEY (product_id)REFERENCES product(id)''')
